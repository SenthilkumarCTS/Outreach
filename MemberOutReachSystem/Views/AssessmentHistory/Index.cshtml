@model IEnumerable<MemberOutReachSystem.Domain.AssessmentHistory>

<h1> Assessment Saved SuccessFully </h1>
<hr />

<br />
<br />

<h4> Assessment History </h4>
<hr />

<br />

<div class="table-responsive">
    <table id="MemberDetails" class="table table-striped table-bordered dt-responsive nowrap">
        <caption>MemberDetails</caption>
        <thead>
            <tr>
                <th scope="col"> Member ID </th>
                <th scope="col"> Disease Name </th>
                <th scope="col"> Assessment ID </th>
                <th scope="col"> Assessment Date </th>  
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var row in Model)
            {
            <tr>
                <td>@Html.DisplayFor(modelItem => row.Member_Id)</td>
                <td>@Html.DisplayFor(modelItem => row.Disease)</td>
                <td>@Html.DisplayFor(modelItem => row.Assessment_ID)</td>
                <td>@Html.DisplayFor(modelItem => row.Insert_DTS)</td>
                <td><input type="button" class="btn btn-success" value="View" name="outreach" onclick="location.href='@Url.Action("Index", "AssessmentView",new { mbrid = row.Member_Id,AssessmentID = row.Assessment_ID })'" /></td>
            </tr>
            }
        </tbody>

    </table>

</div>

<input value="Home" type="button" class="btn btn-success float-right" onclick="location.href='@Url.Action("Index", "Home")'"  />